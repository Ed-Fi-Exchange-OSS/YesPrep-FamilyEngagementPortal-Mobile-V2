<?xml version='1.0' encoding='utf-8'?>
<widget id="org.yesprep.familyengagementapp" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>YES Prep Family Engagement</name>
    <description>
        Family Engagement Mobile App.
    </description>
    <author email="info@student1.org" href="hhttps://www.yesprep.org/">
        Yes Prep Public Schools
    </author>
    <content src="index.html" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="orientation" value="portrait" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <preference name="android-targetSdkVersion" value="30" />
        <preference name="android-minSdkVersion" value="24" />
        <preference default="27.+" name="ANDROID_SUPPORT_V4_VERSION" />
        <preference name="OverrideUserAgent" value="Chrome/88.0.4324.104 Mozilla/5.0 (Linux; Android 9; SM-G960F Build/PPR1.180610.011; wv)" />
        <preference name="loadUrlTimeoutValue" value="100000" />
        <framework src="com.android.support:supportv4:$ANDROID_SUPPORT_V4_VERSION" />
        <icon density="mdpi" src="res/icon/android/drawable-mdpi/ic_yes_prep.png" />
        <icon density="hdpi" src="res/icon/android/drawable-hdpi/ic_yes_prep.png" />
        <icon density="xhdpi" src="res/icon/android/drawable-xhdpi/ic_yes_prep.png" />
        <icon density="xxhdpi" src="res/icon/android/drawable-xxhdpi/ic_yes_prep.png" />
        <icon density="xxxhdpi" src="res/icon/android/drawable-xxxhdpi/ic_yes_prep.png" />
