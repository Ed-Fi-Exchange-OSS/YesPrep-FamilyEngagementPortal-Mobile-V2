        <icon height="1024" platform="ios" src="res/icon/ios/icon-1024.png" width="1024" />
        <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <feature name="CDVWKWebViewEngine">
            <param name="ios-package" value="CDVWKWebViewEngine" />
        </feature>
        <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
        <allow-navigation href="yesprepfamilyengagement://*" />
        <preference name="scheme" value="yesprepfamilyengagement" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <access launch-external="yes" origin="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-device" spec="^2.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="cordova-plugin-http" spec="^1.2.0" />
    <plugin name="cordova-plugin-fingerprint-aio" spec="~3.0.1">
        <variable name="FACEID_USAGE_DESCRIPTION" value="Use Face ID to log into Family Engagement faster" />
    </plugin>
    <plugin name="phonegap-plugin-push" spec="^2.3.0">
        <variable name="SENDER_ID" value="459661466842" />
    </plugin>
    <plugin name="cordova-support-google-services" spec="^1.4.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="^5.0.1" />
    <gap:plugin name="cordova-plugin-customurlscheme" source="npm">
        <param name="URL_SCHEME" value="yesprepfamilyengagement" />
    </gap:plugin>
    <plugin name="cordova-plugin-ios-camera-permissions" spec="https://github.com/Cordobo/cordova-plugin-ios-camera-permissions.git" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="cordova-plugin-app-version" spec="^0.1.9" />
    <plugin name="cordova-plugin-sign-in-with-apple" spec="^0.1.2" />
</widget>
